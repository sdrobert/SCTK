# **********************************************************
# Makefile for rfilter1 Directory
# **********************************************************
SHELL = /bin/sh
PREFIX=
bindir = $(PREFIX)/bin
T=../test_suite

all: tanweenFilt.pl

tanweenFilt.pl: 
	chmod +x tanweenFilt.pl

setTests: all
	@perl TanweenFiltTests.pl -i $(T) -s

check: all
	@echo "Testing tanweenFilt.pl"
	@perl TanweenFiltTests.pl -i $(T)
	@echo "   tanweenFilt.pl tests successfully completed"

install: all
	cp tanweenFilt.pl $(DESTDIR)$(bindir)

clean:

distclean: clean
	$(RM) -r *~ makefile
